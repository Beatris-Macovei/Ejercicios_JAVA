String nombre;
StringTokenizer iniciales = new StringTokenizer("nombre", " ");
while(iniciales.hasMoreTokens())
{
    String palabra = iniciales.nextToken();
    iniciales = iniciales + palabra.charAt(0);
    iniciales = iniciales + ".";
}
return iniciales;

